  <a href="/admin/posts/<%= post.id %>/edit">Editar</a>
  <form
    action="/admin/posts/<%= post.id %>/delete"
    method="POST"
    style="display: inline"
  >
    <button
      type="submit"
      onclick="return confirm('Â¿Seguro que quieres eliminar este post?')"
    >
      Eliminar
    </button>
  </form>
</div>
<% }) %>
